{
  "name": "audako powertoys",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "build/background.js"
  },
  "permissions": ["storage", "webRequest", "scripting", "webNavigation", "notifications"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [{
    "resources": ["build/openInSameTab.js","build/signalChangedListener.js", "build/content.js", "build/content.css"],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "build/assets/favicon.ico",
      "32": "build/assets/favicon.ico"
    }
  }
}