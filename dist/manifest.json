{
  "name": "audako powertoys",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "build/background.js"
  },
  "permissions": ["storage", "webRequest", "scripting", "webNavigation"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [{
    "resources": ["build/inject.js", "build/content.js", "build/content.css"],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "build/popup.html"
  }
}